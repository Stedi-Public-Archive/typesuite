{
  "name": "@stedi/typesuite",
  "version": "0.4.0",
  "description": "TypeScript client for NetSuite SuiteTalk SOAP API",
  "keywords": [
    "TypeScript",
    "NetSuite",
    "SuiteTalk"
  ],
  "author": "Stedi Engineering <engineering@stedi.com>",
  "homepage": "https://github.com/StediInc/TypeSuite#README.md",
  "license": "MIT",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StediInc/TypeSuite.git"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "build": "tsc",
    "test": "tsc && LOG_LEVEL=WARN jest --runInBand --testMatch '**/*.test.ts' && depcheck .",
    "tsc": "tsc"
  },
  "devDependencies": {
    "@commitlint/cli": "9.0.1",
    "@commitlint/config-conventional": "9.0.1",
    "@types/jest": "26.0.3",
    "@types/node": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "3.5.0",
    "@typescript-eslint/parser": "3.5.0",
    "cz-conventional-changelog": "3.2.0",
    "depcheck": "0.9.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint": "7.3.1",
    "husky": "4.2.5",
    "jest": "25.5.4",
    "prettier": "2.0.5",
    "ts-jest": "25.5.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "got": "^11.3.0",
    "jsonix": "^3.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}